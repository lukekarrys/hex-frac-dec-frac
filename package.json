{
  "name": "hex-frac-dec-frac",
  "description": "Convert a hexadecimal fraction to a decimal fraction.",
  "version": "1.0.3",
  "author": "Luke Karrys",
  "bugs": {
    "url": "https://github.com/lukekarrys/hex-frac-dec-frac/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-tape-runner": "^2.0.1",
    "git-validate": "^2.1.4",
    "standard": "^8.0.0-beta.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "files": [
    "lib/*"
  ],
  "homepage": "https://github.com/lukekarrys/hex-frac-dec-frac",
  "keywords": [
    "decimal fraction",
    "hex to dec",
    "hexadecimal fraction",
    "hexadecimal fraction to decimal fraction"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "pre-commit": [
    "lint",
    "test",
    "validate"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:lukekarrys/hex-frac-dec-frac.git"
  },
  "scripts": {
    "compile": "babel src --out-dir lib",
    "lint": "standard",
    "prepublish": "npm run compile",
    "test": "babel-tape-runner test/*.js | tap-spec",
    "validate": "npm ls"
  },
  "standard": {
    "ignore": [
      "**/lib/**"
    ]
  }
}
